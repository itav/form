{% if data.error is defined %}<span class="error">{{ data.error }}</span>{% endif %}
<label {% if data.id is defined %} for="{{ data.id }}" {% elseif data.name is defined %} for="{{ data.name }}" {% endif %}>
    {% if data.label is defined %} {{ data.label }}{% endif %}
</label>
<input {{ include('formelement.twig') }}
    {%if data.name%} name="{{data.name}}"{% endif %} 
    {%if data.type%} type="{{data.type}}"{% endif %} 
    {%if data.value%} value="{{data.value}}"{% endif %}
    {%if data.disabled%} disabled="disabled"{% endif %}
    {%if data.checked%} checked="checked"{% endif %} 
    {%if data.size%} size="{{data.size}}"{% endif %}>    
