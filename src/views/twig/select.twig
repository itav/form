<label {% if data.id is defined %} for="{{ data.id }}" {% elseif data.name is defined %} for="{{ data.name }}" {% endif %}>
    {% if data.label is defined %} {{ data.label }}{% endif %}
</label>
<select {{ include('formelement.twig') }} {% if data.name is defined %}name="{{ data.name }}"{% endif %} {% if data.multiple is defined %} multiple="multiple" {% endif %}>
    {% for option in data.options %}
        {{ include(option.template, {data: option}, with_context = false) }}
    {% endfor %}    
</select>
